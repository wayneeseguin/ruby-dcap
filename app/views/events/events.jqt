<script type="text/x-jquery-template" title="events">
  <h1>Events</h1>
  <% $.each(_context, function(index, event){ %>
    <div class="event" event_id="<%= event.id %>"><%= event.name %></div>
  <% }); %>
</script>

<script type="text/x-jquery-template" title="event">
  <h1>Event <%= name %></h1>
  <p><%= description %></p>
</script>
